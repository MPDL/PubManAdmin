{
    "env": {
        "browser": true,
        "es2021": true
    },
    "extends": [
        "google"
    ],
    "parser": "@typescript-eslint/parser",
    "parserOptions": {
        "ecmaVersion": 2015,
        "sourceType": "module"
    },
    "plugins": [
        "@typescript-eslint"
    ],
    "rules": {
        "indent": ["error", 2],
        "linebreak-style": ["error", "windows"],
        "max-len":["error", {"ignoreComments":true}, {"code":250}],
        "new-cap": 0,
        "require-jsdoc":0
    }
}
